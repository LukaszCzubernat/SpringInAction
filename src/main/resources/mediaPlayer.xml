<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="compactDisc" class="ogre.soundsystem.BlankDisc">
        <constructor-arg value="Sgt. Peppers Lonely Hearts Club Band"/>
        <constructor-arg value="The Beatles"/>
        <constructor-arg>
            <list>
                <value></value>
            </list>
        </constructor-arg>
    </bean>

    <!--<bean id="compactDisc" class="ogre.soundsystem.blankDisc" c:title="Sgt. Peppers Lonely Hearts Club Band" c:author="The Beatles"/>-->
    <!--<bean id="compactDisc" class="ogre.soundsystem.blankDisc" c:_0="Sgt. Peppers Lonely Hearts Club Band" c:_1="The Beatles"/>-->

    <bean id="cdPlayer" class="ogre.soundsystem.CDPlayer">
        <constructor-arg ref="compactDisc"/>
    </bean>

    <!--<bean id="cdPlayer" class="ogre.soundsystem.CDPlayer" c:compactDisc="compactDisc"/>-->
    <!--<bean id="cdPlayer" class="ogre.soundsystem.CDPlayer" c:_0-ref="compactDisc"/>-->
</beans>